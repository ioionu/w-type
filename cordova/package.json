{
  "name": "r-type",
  "displayName": "r-type",
  "version": "2.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "run": "cordova run browser",
    "build-dev": "cordova build browser --debug",
    "build-apk-dev": "cordova build android",
    "build-apk-prod": "cordova build android",
    "serve": "cordova serve",
    "reinstall-apk-dev": "adb uninstall com.sydneywebdev.wtype && adb install platforms/android/app/build/outputs/apk/debug/app-debug.apk",
    "reinstall-apk-prod": "adb uninstall com.sydneywebdev.wtype && adb install platforms/android/app/build/outputs/apk/release/app-release.apk"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Joshua McCluskey",
  "license": "GNU GENERAL PUBLIC LICENSE Version 3",
  "dependencies": {
    "cordova-android": "^8.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-fullscreen": "^1.2.0",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "es6-promise-plugin": "^4.2.2"
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-screen-orientation": {},
      "cordova-plugin-fullscreen": {}
    },
    "platforms": [
      "android",
      "browser"
    ]
  }
}